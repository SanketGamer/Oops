package Interface;


class Calculator{
    -- method overloading->can accept same name,diff peramiter, but can not accept diff return type and diff argument means(double a,int b) wrong
    public int add(int n1,int n2,int n3){
        int res=n1+n2+n3;
        return res;
    }
    public int add(int n1,int n2){
        return n1+n2;
    }

}
public class Practice {
    public static void main(String[] args) {
        Calculator obj=new Calculator();
        int r1=obj.add(3, 4, 2);
        int r2=obj.add(12, 12);
        System.out.println(r1);
        System.out.println(r2);
    }
}
