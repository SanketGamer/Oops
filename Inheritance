package Interface;

//single inheritance - A->B
//multilevel inheritance - A->B->C->D (one by one)
//multiple inheritance - A(parent)    B(parent)
                          \  /
                           C(child)

EX:
//we can no use multiple inheritance in java because if 2 parent have same methods how i choose one thats why we can not use.this called Amguity problem
class A{

}
class B{

}
class C extends A,B{  //not possible

}

//Inheritance

class Calc{

  public int add(int n1,int n2){
    return n1+n2;
  }
  public int sub(int n1,int n2){
    return n1-n2;
  }

}

class AdvCalc extends Calc{
  
  public int mul(int n1,int n2){
    return n1*n2;
  }
  public int div(int n1,int n2){
    return n1%n2;
  }
}


public class Practice {
    public static void main(String[] args) {
      Calc obj=new Calc();
      AdvCalc obj1=new AdvCalc();
      int r1=obj1.add(4,5);
      int r2=obj1.sub(5, 4);
      //System.err.println(r1+" "+r2);
      int r3=obj1.mul(4,5);
      int r4=obj1.div(6, 2);
      System.err.println(r1+" "+r2+" "+r3+" "+r4);
    }
}


