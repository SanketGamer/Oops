package Interface;

//that class B is totally depended A class we use inner class

class A{
    int age;
    public void show(){
       System.out.println("in show");
    }
class B{

    public void config(){
      System.out.println("Config");
    }
}

}


public class Practice {
    public static void main(String[] args) {

        A obj=new A();
        obj.show();
      
        A.B obj1=obj.new B();
        obj1.config();


    }
}




